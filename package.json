        "dotenv": "^8.2.0",
        "eslint": "7.14.0",
        "eslint-plugin-jsdoc": "30.7.8",
        "mocha": "8.2.1",
        "mocha-steps": "^1.3.0",
        "nock": "^13.0.5",
        "prettier": "2.1.2",
        "ts-node": "9.0.0",
        "typedoc": "0.19.2",
        "typescript": "4.0.5"
    },
    "files": [
        "dist"
