{
    "name": "saturnjs",
    "version": "1.0.0",
    "description": "The unofficiial Saturn API Wrapper",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "https://github.com/k2on/saturnjs.git",
    "author": "Max Koon <maxk@nix2.io>",
    "license": "MIT",
    "private": false,
    "prettier": "@nix2/prettier-config",
